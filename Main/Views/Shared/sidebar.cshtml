@model sql4.Models.ad

@Styles.Render("~/Content/comPk.css")
@Styles.Render("~/Content/link.css")
@Styles.Render("~/Content/Pk.css")



<br/>
<div id="list_item_thumbs" >
    <div class="item">
        <a class="item_link" href="/ad/Details/@Model.ad_id"></a>
        <h6 class="ellipsis item_subject">
            <a href="">  Catagory  </a>:  @Model.catagory
        </h6>

        <h6 class="ellipsis item_subject">
            <a href="">
                Title: </a>
             @Model.title
            </h6>
        <h6 class="ellipsis item_subject">
            <a href="">
                Price:
            </a>
            @Model.price.ToString("#,##0")
        </h6>

        <h6 class="ellipsis item_subject">
            <a href="">
                City:
            </a>
            @Model.city.cityName
        </h6>




    </div>
</div>





